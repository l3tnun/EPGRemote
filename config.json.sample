        "index.php" : "index2.php",
        "hourheight" : 180,
        "videoPath" : "/var/www/epgrec/video",
        "thumbsPath" : "/var/www/epgrec/thumbs"
    },

    "video" : [
        {
            "id" : "1",
            "size" : "1280x1720",
            "vb" : "3000k",
            "ab" : "192k",
            "audioMode" : "main"
        },
        {
            "id" : "2",
            "size" : "960x540",
            "vb" : "2000k",
            "ab" : "192k",
            "audioMode" : "sub"
        },
        {
            "id" : "3",
            "size" : "720x480",
            "vb" : "1500k",
