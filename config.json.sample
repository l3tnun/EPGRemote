            "audioMode" : "sub"
        }
    ],

    "tuners": [
        {
            "id" : "1",
            "name"      : "PT3-S1",
            "types"     : [ "BS", "CS" ],
            "command"   : "/hoge/recpt1 --device /dev/pt3video0 --b25 --strip --sid <sid> <channel> - - "
        },
        {
            "id" : "2",
            "name"      : "PT3-S2",
            "types"     : [ "BS", "CS" ],
            "command"   : "/hoge/recpt1 --device /dev/pt3video1 --b25 --strip --sid <sid> <channel> - - "
